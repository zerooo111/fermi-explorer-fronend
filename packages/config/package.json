{
  "name": "@fermi/config",
  "version": "1.0.0",
  "description": "Shared configuration for Fermi Explorer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./backend": {
      "types": "./dist/backend.d.ts",
      "import": "./dist/backend.js"
    },
    "./frontend": {
      "types": "./dist/frontend.d.ts",
      "import": "./dist/frontend.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "config",
    "environment",
    "fermi-explorer"
  ]
}